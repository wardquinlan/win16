IDD_CREATE_RULE DIALOG 8, 8, 248, 248
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Create Trading Rule"
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT           "Rule Name:", -1, 8, 8, 96, 8
    COMBOBOX        IDC_RULE, 8, 16, 96, 112, CBS_DROPDOWN | CBS_SORT | 
                    WS_VSCROLL | WS_TABSTOP
    
    LTEXT           "Entry:", -1, 8, 48, 112, 8
    EDITTEXT        IDC_BUY, 8, 56, 112, 56, ES_MULTILINE | ES_AUTOVSCROLL | 
                    ES_AUTOHSCROLL | ES_WANTRETURN
    
    LTEXT           "Entry Filter:", -1, 128, 48, 112, 8
    EDITTEXT        IDC_FILTER, 128, 56, 112, 56, ES_MULTILINE | ES_AUTOVSCROLL | 
                    ES_AUTOHSCROLL | ES_WANTRETURN

    LTEXT           "Exit:", -1, 8, 120, 112, 8
    EDITTEXT        IDC_SELL, 8, 128, 112, 56, ES_MULTILINE | ES_AUTOVSCROLL | 
                    ES_AUTOHSCROLL | ES_WANTRETURN
    
    LTEXT           "Exit Filter:", -1, 128, 120, 112, 8
    EDITTEXT        IDC_FILTER2, 128, 128, 112, 56, ES_MULTILINE | ES_AUTOVSCROLL | 
                    ES_AUTOHSCROLL | ES_WANTRETURN
                     
    GROUPBOX        "Enter/Exit on:", -1, 8, 192, 96, 48
    CONTROL         "Current Close", IDC_CURRENT_CLOSE, "Button", 
                    BS_AUTORADIOBUTTON | WS_TABSTOP, 16, 208, 64, 10
    CONTROL         "Next Open", IDC_NEXT_OPEN, "Button", 
                    BS_AUTORADIOBUTTON | WS_TABSTOP, 16, 224, 64, 10

    PUSHBUTTON      "C&lose", IDCANCEL, 192, 16, 48, 14
    PUSHBUTTON      "&Update", IDOK, 192, 32, 48, 14
END

