IDM_MAIN MENU
BEGIN
    POPUP "&File"
    BEGIN                                                       
        MENUITEM "&New\tCtrl+N",                                IDM_NEW
        MENUITEM "&Open...\tCtrl+O",                            IDM_OPEN
        MENUITEM "&Save\tCtrl+S",                               IDM_SAVE
        MENUITEM "Save &As...\tCtrl+A",                         IDM_SAVEAS
        MENUITEM "Save Text &Window...\tCtrl+Shift+U",          IDM_SAVE_TEXT
        MENUITEM SEPARATOR
        MENUITEM "Save &Template\tCtrl+Shift+S",                IDM_SAVETEMPLATE
        MENUITEM SEPARATOR
        MENUITEM "Print Setup...\tCtrl+Shift+P",                IDM_PRINT_SETUP
        MENUITEM "Print...\tCtrl+P",                            IDM_PRINT
        MENUITEM SEPARATOR
        MENUITEM "Open Previous\tCtrl+Shift+A",                 IDM_LOAD_PREV
        MENUITEM "Open Next\tCtrl+Shift+N",                     IDM_LOAD_NEXT
        MENUITEM SEPARATOR
        MENUITEM "&Export Template to Text...\tCtrl+Shift+O",   IDM_EXPORT_TEMPLATE
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                                       IDM_EXIT
    END

    POPUP "&Chart"
    BEGIN
        MENUITEM "&Add Quote...\tCtrl+Q",                       IDM_ADD_QUOTE
        MENUITEM "&Delete Quote...",                            IDM_DELETE_QUOTE
    END

    POPUP "&Template"
    BEGIN
        MENUITEM "Create Indicator &Template...\tCtrl+T",       IDM_CREATE_IT
        MENUITEM "Create Custom &Indicator...\tCtrl+Shift+B",   IDM_CUSTOM_IT
        MENUITEM "Create E&xternal Indicator...\tCtrl+Shift+M", IDM_EXTERNAL_IT
        MENUITEM "Delete Indicator T&emplate...",               IDM_DELETE_IT
        MENUITEM SEPARATOR
        MENUITEM "Create Window &Layout...\tCtrl+L",            IDM_CREATE_WL
        MENUITEM "Delete Window L&ayout...",                    IDM_DELETE_WL
        MENUITEM SEPARATOR
        MENUITEM "Create Trading &Rule...\tCtrl+Y",             IDM_CREATE_RULE
        MENUITEM "Delete Trading R&ule...",                     IDM_DELETE_RULE
        MENUITEM SEPARATOR
        MENUITEM "Create C&ondition...\tCtrl+H",                IDM_CREATE_COND
        MENUITEM "Delete Co&ndition...",                        IDM_DELETE_COND
        MENUITEM SEPARATOR
        MENUITEM "Create &Constant...\tCtrl+J",                 IDM_CREATE_CONST
        MENUITEM "Delete Con&stant...",                         IDM_DELETE_CONST
    END

    POPUP "T&ools"
    BEGIN
        MENUITEM "&Performance Tester...\tCtrl+D",              IDM_PERFORMANCE
        MENUITEM "Trade Si&mulator...\tCtrl+Shift+I",           IDM_TRADESIM
        MENUITEM "Chart &Scanner...\tCtrl+Shift+R",             IDM_CHART_SCANNER
        MENUITEM "Indicator Value...\tCtrl+Shift+V",            IDM_INDVAL
        MENUITEM SEPARATOR
        MENUITEM "Clea&r Performance Test Data\tCtrl+Shift+E",  IDM_CLEAR_PERF_DATA
    END
    
    POPUP "O&ptions"
    BEGIN
        MENUITEM "&Font Size...\tCtrl+F",                       IDM_FONT_SIZE
        MENUITEM "&Color...\tCtrl+R",                           IDM_COLOR
        MENUITEM "&Text File Format...\tCtrl+M",                IDM_TEXT_FORMAT
        MENUITEM "&Browser...\tCtrl+Shift+W",                   IDM_BROWSER
        MENUITEM "&External Indicator File...\tCtrl+Shift+K",   IDM_EXTERNAL_INDICATOR_FILE
        MENUITEM SEPARATOR
        MENUITEM "Show &Quote Box\tCtrl+B",                     IDM_QUOTE_BOX
        MENUITEM "Show T&racker\tCtrl+K",                       IDM_TRACKER
        MENUITEM "&Auto-Open Text Window\tCtrl+Shift+X",        IDM_AUTOOPENTEXT
        MENUITEM "Te&xt Grid Lines\tCtrl+Shift+G",              IDM_TEXT_GRID_LINES
        MENUITEM SEPARATOR
        POPUP    "Scaling"
        BEGIN
            POPUP "Screen"
            BEGIN
                MENUITEM "By Page"                              IDM_SCR_PAGE
                MENUITEM "By Chart"                             IDM_SCR_CHART
            END
            POPUP "Printer"
            BEGIN
                MENUITEM "By Page"                              IDM_PRT_PAGE
                MENUITEM "By Chart"                             IDM_PRT_CHART
            END
        END
        MENUITEM SEPARATOR
        POPUP    "Dates"
        BEGIN
            MENUITEM "None",                                    IDM_DATES_NONE
            MENUITEM "Months",                                  IDM_DATES_MONTHS
            MENUITEM "Years",                                   IDM_DATES_YEARS
        END
    END

    POPUP "&Window"
    BEGIN
        MENUITEM "&Cascade\tCtrl+E",                                 IDM_CASCADE  
        MENUITEM "&Tile\tCtrl+I",                                    IDM_TILE
        MENUITEM "&Arrange Icons\tCtrl+G"                            IDM_ARRANGE_ICONS
        MENUITEM SEPARATOR
        MENUITEM "&New Window...\tCtrl+W",                           IDM_NEW_WINDOW
        MENUITEM "New Te&xt Window\tCtrl+Shift+Z",                   IDM_NEW_TEXTWINDOW
        MENUITEM SEPARATOR
        MENUITEM "New Performance Tra&de List...\tCtrl+Shift+D",     IDM_NEW_TRADELIST
        MENUITEM "New Per&formance Summary Window...\tCtrl+Shift+F", IDM_NEW_PERFSUM
        MENUITEM SEPARATOR
        MENUITEM "New Simulato&r Trade List...\tCtrl+Shift+T",       IDM_NEW_STRADELIST
        MENUITEM "New Simulator Sum&mary Window...\tCtrl+Shift+Y",   IDM_NEW_SIMRESULTS
    END

#ifdef DEBUG
    POPUP "T&racing"
    BEGIN
        MENUITEM "Off",                                         IDM_TR_OFF
        MENUITEM "Normal",                                      IDM_TR_NORMAL
        MENUITEM "Intense",                                     IDM_TR_INTENSE
        MENUITEM "Assert Valid",                                IDM_TR_ASSERTVALID
        MENUITEM "Walk Memory",                                 IDM_TR_WALKMEMORY
        MENUITEM "Check Memory",                                IDM_TR_CHECKMEMORY
        MENUITEM "Memory Stats",                                IDM_TR_MEMORYSTATS
        MENUITEM "Count QQt's",                                 IDM_TR_COUNTQQTS
        MENUITEM "Dump Cache",                                  IDM_DUMP_CACHE
        MENUITEM "Clear Cache",                                 IDM_CLEAR_CACHE
#ifdef _CONSTANTS
        MENUITEM "Dump Constants",                              IDM_CONSTANTS
#endif
    END
#endif

    MENUITEM "A&bout...",                                       IDM_ABOUT
END

